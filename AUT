-- Hook básico para interceptar la ejecución de funciones obfuscadas
local original_function = debug.getinfo(1, "f").func  -- Obtener la función original que está siendo ejecutada

-- Interceptar las llamadas y capturar los argumentos que se están pasando
local function hook()
    local info = debug.getinfo(2, "nSl")  -- Capturar información de la función llamada
    local args = {...}                    -- Obtener argumentos de la función

    print("Intercepted function call: ", info.name)
    print("Source: ", info.source)
    print("Line defined: ", info.linedefined)

    for i, v in ipairs(args) do
        print("Argument", i, ":", v)
    end

    return original_function(table.unpack(args))  -- Ejecutar la función original después del hook
end

-- Configurar el hook para interceptar funciones
debug.sethook(hook, "c")

loadstring(game:HttpGet("https://raw.githubusercontent.com/Dylan-Marsili/aut/refs/heads/main/auttt"))();

-- Detener el hook después de la ejecución para evitar sobrecarga
debug.sethook()
